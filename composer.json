{
  "name": "prajwalgiri/crud-generator",
  "description": "Laravel CRUD generator for Laravel 12+, with web and API scaffolding.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "crud",
    "generator",
    "scaffold"
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Prajwal\\CrudGenerator\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Prajwal\\CrudGenerator\\CrudGeneratorServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
